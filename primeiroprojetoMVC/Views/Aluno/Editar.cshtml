@model Aluno
<form asp-controller="Aluno" asp-action="EditarAluno" method="post">

    <input type ="hidden" asp-for="Id" />

    <div class="mb-3 col-md-2">
        <label class="form-label">Nome</label>
        <input type="text" asp-for="Nome" class="form-control">
    </div>

    <div class="mb-3 col-md-2">
        <label class="form-label">Matrícula</label>
        <input type="text" asp-for="Matricula" class="form-control">
    </div>

    <div class="mb-3 col-md-2">
        <label class="form-label">Cpf</label>
        <input type="text" asp-for="Cpf" class="form-control">
    </div>

    <div class="mb-3 col-md-2">
        <label class="form-label">Data De Nascimento</label>
        <input type="text" asp-for="DataDeNascimento" class="form-control">
    </div>

    <div class="mb-3 col-md-2">
        <label class="form-label">Endereço</label>
        <input type="text" asp-for="Endereco" class="form-control">
    </div>

    <div class="text-left">
        <button type="submit" class="btn btn-primary">Editar</button>
    </div>
</form>